<div class="container">
  <div class="page-header">
    <div class="getback">&larr; <%=link_to 'Properties', properties_path %></div>
    <h1>Edit Property</h1>
  </div>
  <div class="row">
    <div class="col-xs-12 .col-sm-6 .col-md-8">
      <%=form_for @property, url: property_path(@property), html: { multipart: true } do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
        <p class="submit"><%=f.submit 'Update Property' %></p>
      <% end %>
    </div>
  </div>
</div>