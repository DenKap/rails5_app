<div class="container">
  <div class="page-header">
    <h1>New Property</h1>
  </div>
  <div class="row">
    <div class="col-xs-12 .col-sm-6 .col-md-8">
      <%=form_for @property, url: properties_path(@property), html: { multipart: true } do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
        <p class="submit"><%=f.submit 'Add New Property' %></p>
      <% end %>
    </div>
  </div>
</div>
